{
    "name": "miguelavaqrod/newsletter",
    "description": "An extension to save newsletter email subscriptions.",
    "type": "bolt-extension",
    "keywords": [
        "newsletter"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Miguel Angel Vaquero",
            "email": "miguela.vaqrod@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Miguelavaqrod\\Newsletter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\Miguelavaqrod\\Newsletter\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-class": "Bolt\\Extension\\Miguelavaqrod\\Newsletter\\NewsletterExtension"
    }
}
